<% provide(:title, @course.title) %>
<div class="course-show">
	<h1>
		<%= @course.title %>
	</h1>
	<% if creator? %>
		<div class="edit-btn profile-btn">
			<%= link_to "Edit Course", edit_course_path(@course) %>
		</div>
	<% end %>
	<div>
		<%= @course.description %>
	</div>
	<div>
		Files
	</div>
	<% if creator? %>
		<%= render 'shared/note_form' %>
	<% end %>
	<div>
		<% if @notes.any? %>
			<% if !@notes.nil? %>
					<ol class="notes">
						<%= render @notes %>
					</ol>
			<% end %>
		<% else %>
		<% end %>
	</div>
</div>